@model IEnumerable<Real_estate_business.Models.Rent> 

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PropertyNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Owner.OwnerfNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Staff.StaffNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch.BranchNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Street)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ptype)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rent1)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PropertyNo)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.OwnerNoRef)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StaffNoRef)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BranchNoRef)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Street)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ptype)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rooms)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rent1)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.PropertyNo }) |
                    @Html.ActionLink("Details", "Details", new { id = item.PropertyNo }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.PropertyNo })
                </td>
            </tr>
        }

    </table>
</body>
</html>
